<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      layout="admin-2columns-left"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="left">
            <block class="Mymodule\Test\Block\Adminhtml\Link\Edit\Tabs"
                   name="mymodule_link.edit.tabs"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block
                    class="Mymodule\Test\Block\Adminhtml\Link\Edit"
                    name="mymodule_test_link.edit"
            />
        </referenceContainer>
        <referenceContainer name="js">
            <block class="Magento\Backend\Block\Template" name="adminhtml.link.page.grid.js" template="Mymodule_Test::link/page_grid_js.phtml"/>
        </referenceContainer>
    </body>
</page>